<% @restaurant_types.each do |t| %>
                                                                                                                                    <tr>
                                                                        <th style="text-align: center"><%= t.id %></th>
                                                                        <th style="text-align: center"><%= t.name %></th>
                                                                        <th style="text-align: center">
                                                                              <%= link_to "<i class='material-icons'>edit</i>".html_safe,restaurant_type_edit_path(t), method: :get %>
                                                                               
                                                                               <%= link_to "<i class='material-icons'>delete</i>".html_safe, restaurant_type_destroy_path(t), method: :delete, data: {confirm: "Bạn chắc chắn chứ?"} %>
                                                                                
                                                                        </th>
                                                                  </tr>

                                                                  
                                                                  <% end %>