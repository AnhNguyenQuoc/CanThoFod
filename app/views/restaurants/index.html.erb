      <header class="header" style="background-image: none;">
            <div class="header-bar">
                  <nav>
                        <ul id="dropdownnavmenu" class="dropdown-content">
                              <li>
                              <%= link_to "Đăng xuất", logout_path, method: :delete%>
                              </li>

                        </ul>
                        <div class="nav-wrapper">
                              <%= link_to 'Logo', root_path, class: 'brand-logo' %>
                              <a href="#" data-activates="mobile-demo" class="button-collapse">
                                    <i class="material-icons">menu</i>
                              </a>
                              <ul class="right hide-on-med-and-down">
                                    <% if logged_in?%>

                                          <% if current_user.role == 1 && !current_user.restaurant %>           
                                    <li>
                                          <%= link_to "Tạo Cửa hàng", new_restaurant_path %>
                                    </li>
                                          <% elsif current_user.role == 0%>
                                          <li><%= link_to "Cửa hàng", restaurants_path%></li>
                                          <% end %>
                                          
                                    <li>
                                         <a class="dropdown-button" href="#" data-activates="dropdownnavmenu"><%= current_user.username%><i class="material-icons right">arrow_drop_down</i></a>
                                    </li>
                                    <% else %>
                                          <li>
                                                <%= link_to "Đăng nhập", login_path %>
                                           </li>
                                    <% end %>
                              </ul>
                        </div>
                  </nav>
 
                  <ul class="side-nav" id="mobile-demo">
                        <li>
                              <a href="sass.html">Cửa hàng</a>
                        </li>
                         <% if logged_in? %>
                              <li>
                                    <%= link_to "Đăng xuất", logout_path, method: :delete%>
                              </li>
                        <% else %>
                              <li>
                                    <%= link_to "Đăng nhập", login_path %>
                              </li>
                        <% end %>
                  </ul>
                  <div class="background-list-restaurant">
                        <div class="container">
                              <div class="row">
                                    <nav>
                                          <div class="nav-wrapper">
                                                <div class="col s12">
                                                      <%= link_to "Home",root_path, class: "breadcrumb"%>
                                                      <%= link_to "Cửa hàng",restaurants_path, class: "breadcrumb"%>
                                                </div>
                                          </div>
                                    </nav>
                              </div>
                              <div class="input-field col s12">
                                    <!-- Dropdown Trigger -->
                                    <a class='dropdown-button btn'  data-activates='dropdown1'>Chọn Quận</a>

                                    <!-- Dropdown Structure -->
                                    <ul id='dropdown1' class='dropdown-content'>
                                          <li>
                                                <a>Quận Ninh Kiều</a>
                                          </li>
                                          <li>
                                                <a>Quận Bình Thủy</a>
                                          </li>
                                          <li>
                                                <a>Quận Cái Răng</a>
                                          </li>
                                          <li>
                                                <a>Quận Ô Môn</a>
                                          </li>
                                          <li>
                                                <a>Quận Thốt Nốt</a>
                                          </li>
                                    </ul>
                              </div>
                              <div class="row">
                                    <div class="input-field col s12 m4">
                                          <a class='dropdown-button btn' data-activates='loaicuahang'>Chọn Loại Cửa Hàng</a>


                                          <ul id='loaicuahang' class='dropdown-content'>
                                                <li>
                                                      <a>Quận Ninh Kiều</a>
                                                </li>
                                                <li>
                                                      <a>Quận Bình Thủy</a>
                                                </li>
                                                <li>
                                                      <a>Quận Cái Răng</a>
                                                </li>
                                                <li>
                                                      <a>Quận Ô Môn</a>
                                                </li>
                                                <li>
                                                      <a>Quận Thốt Nốt</a>
                                                </li>
                                          </ul>
                                    </div>
                                    <div class="input-field col s12 m4">
                                          <a class='dropdown-button btn' data-activates='loaithucan'>Chọn Loại Thức ăn</a>


                                          <ul id='loaithucan' class='dropdown-content'>
                                                <li>
                                                      <a>Quận Ninh Kiều</a>
                                                </li>
                                                <li>
                                                      <a>Quận Bình Thủy</a>
                                                </li>
                                                <li>
                                                      <a>Quận Cái Răng</a>
                                                </li>
                                                <li>
                                                      <a>Quận Ô Môn</a>
                                                </li>
                                                <li>
                                                      <a>Quận Thốt Nốt</a>
                                                </li>
                                          </ul>
                                    </div>
                                    <div class="input-field col s12 m4">
                                          <a class='dropdown-button btn' data-activates='sapxep'>Sắp xếp</a>


                                          <ul id='sapxep' class='dropdown-content'>
                                                <li>
                                                      <a>Từ A - Z</a>
                                                </li>
                                                <li>
                                                      <a>Theo Nhận Xét</a>
                                                </li>
                                          </ul>
                                    </div>
                              </div>
                              <div class="row">
                                    <form class="col s12">
                                          <div class="row">
                                                <div class="input-field col s12">
                                                      <i class="material-icons prefix">search</i>
                                                      <input id="icon_prefix" type="text" class="validate">
                                                      <label for="icon_prefix">Tìm kiếm</label>
                                                </div>
                                          </div>

                                    </form>
                              </div>

                        </div>
                  </div>
            </div>
      </header>



            <section class="list-restaurant">
            <div class="container">
                  <div class="row">
                        
                        <% @restaurants.each do |restaurant| %>
                        <div class="col m4 s12">
                              <div class="restaurant-card card" style="width: 100%;">
                                    <div class="card-image">
                                          <%= image_tag restaurant.image, class: "card-img-top", alt: restaurant.image.filename, style: "height: 200px;" %>
                                          <span class="card-title"><%= restaurant.name%></span>
                                    </div>
                                    <div class="card-content">
                                          <p><%= restaurant.description %></p>
                                          <p><%= restaurant.timeopen.strftime('%H:%M') %>  - <%= restaurant.timeclose.strftime('%H:%M') %></p>

                                    </div>
                                    <div class="card-action">
                                          <%= link_to "Đi đến cửa hàng",  restaurant_dat_mon_path(restaurant) , class: "btn waves-effect waves-light", style: "width: 100%"%>
                                    </div>
                              </div>
                        </div>
                        <% end %>
                        
                  </div>
            </div>
      </section>



      <%= render 'layout_extends/footer' %>