      <header class="header" style="background-image: none; height: 55%">
            <div class="header-bar">
                  <nav>
                        <ul id="dropdownnavmenu" class="dropdown-content">
                              <% if logged_in? %>
                                    <li>
                                          <%= link_to "Quản lý thông tin cá nhân", edit_user_path(current_user)%>
                                    </li>
                                    <li>
                                          <%= link_to "Đăng xuất", logout_path, method: :delete%>
                                    </li>
                              <% end %>
                        </ul>
                        <div class="nav-wrapper">
                              <%= link_to 'Logo', root_path, class: 'brand-logo' %>
                              <a href="#" data-activates="mobile-demo" class="button-collapse">
                                    <i class="material-icons">menu</i>
                              </a>
                              <ul class="right hide-on-med-and-down">
                                    <% if logged_in?%>

                                          <% if current_user.role.id == 2 && !current_user.restaurant %>           
                                    <li>
                                          <%= link_to "Tạo Cửa hàng", new_restaurant_path %>
                                    </li>
                                          <% elsif current_user.role.id == 1%>
                                          <li><%= link_to "Cửa hàng", restaurants_path%></li>
                                          <% end %>
                                          
                                    <li>
                                         <a class="dropdown-button" href="#" data-activates="dropdownnavmenu"><%= current_user.username%><i class="material-icons right">arrow_drop_down</i></a>
                                    </li>
                                    <% else %>
                                          <li>
                                                <%= link_to "Đăng nhập", login_path %>
                                           </li>
                                    <% end %>
                              </ul>
                        </div>
                  </nav>
 
                  <ul class="side-nav" id="mobile-demo">
                        <li>
                              <a href="sass.html">Cửa hàng</a>
                        </li>
                         <% if logged_in? %>
                              <li>
                                    <%= link_to "Đăng xuất", logout_path, method: :delete%>
                              </li>
                        <% else %>
                              <li>
                                    <%= link_to "Đăng nhập", login_path %>
                              </li>
                        <% end %>
                  </ul>
                  <div class="background-list-restaurant">
                        <div class="container" style="width: 80%">
                              <div class="row">
                                    <nav>
                                          <div class="nav-wrapper">
                                                <div class="col s12">
                                                      <%= link_to "Home",root_path, class: "breadcrumb"%>
                                                      <%= link_to "Cửa hàng",restaurants_path, class: "breadcrumb"%>
                                                </div>
                                          </div>
                                    </nav>
                              </div>
                              <div class="input-field col s12">
                                    <!-- Dropdown Trigger -->
                                    <a class='dropdown-button btn'  data-activates='dropdown1' >Chọn Quận</a>

                                    <!-- Dropdown Structure -->
                                    <ul id='dropdown1' class='dropdown-content'>
                                          <li>
                                                <a>Tất cả</a>
                                          </li>
                                          <% District.all.each do |district|%>
                                          <li>
                                                <%= link_to district.name, restaurants_path(:find_district => district.id)%>
                                          </li>
                                          <% end %>
                                    </ul>
                              </div>
                              <div class="row">
                                    <div class="input-field col s12 m4">
                                          <a class='dropdown-button btn' data-activates='loaicuahang' style="width: 100%;">Chọn Loại Cửa Hàng</a>


                                          <ul id='loaicuahang' class='dropdown-content'>
                                                <li>
                                                      <a>Quận Ninh Kiều</a>
                                                </li>
                                                <li>
                                                      <a>Quận Bình Thủy</a>
                                                </li>
                                                <li>
                                                      <a>Quận Cái Răng</a>
                                                </li>
                                                <li>
                                                      <a>Quận Ô Môn</a>
                                                </li>
                                                <li>
                                                      <a>Quận Thốt Nốt</a>
                                                </li>
                                          </ul>
                                    </div>
                                    
                                    <div class="input-field col s12 m4 offset-m4">
                                          <a class='dropdown-button btn' data-activates='sapxep' style="width: 100%;">Sắp xếp</a>


                                          <ul id='sapxep' class='dropdown-content'>
                                                <li>
                                                      <%= link_to "Theo nhận xét", restaurants_path(:order_rating => "Theo nhận xét")%>
                                                </li>
                                                <li>
                                                     <%= link_to "Từ A - Z", restaurants_path(:order_name => "Từ A - Z")%>
                                                </li>
                                          </ul>
                                    </div>
                              </div>
                              <div class="row">
                                    <%= form_tag cua_hang_path,remote: true,method: :get,:id => 'form-search' do |f| %> 
                                          <div class="row">
                                                
                                                <div class="input-field col s10">
                                                            <%= text_field_tag :starts_with, params[:starts_with] %>
                                                            <%= label_tag 'starts_with', 'Tìm kiếm'%>
                                                      </div>
                                                      <div class="input-field col s2">
                                                       
                                                            <%= button_tag(class: "btn", style: "width: 94%", id: "btn-search") do %>
                                                                  <i class="material-icons">search</i>
                                                            <%end%>
                                                      </div> 
                                                </div>
                                                
                                          </div>
                                    <% end %>
                              </div>

                        </div>
                  </div>
            </div>
      </header>



            <section class="list-restaurant">
            <div class="container" style="width: 80%">
                  <div class="row" >
        <div id="restaurants">
                        <%= render 'restaurants' %>
                        </div>
                  </div>
            </div>
      </section>



      <%= render 'layout_extends/footer' %>