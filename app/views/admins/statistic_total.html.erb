 <%# <meta http-equiv="Refresh" content="30"> %>
 <!-- Start Page Loading -->
      <div id="loader-wrapper">
            <div id="loader"></div>
            <div class="loader-section section-left"></div>
            <div class="loader-section section-right"></div>
      </div>
      <!-- End Page Loading -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START HEADER -->
     <%= render 'admin_restaurants/header' %>
      <!-- END HEADER -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START MAIN -->
      <div id="main">
            <!-- START WRAPPER -->
            <div class="wrapper">
                  <!-- START LEFT SIDEBAR NAV-->
                  <%= render 'sidebar' %>
                  <!-- END LEFT SIDEBAR NAV-->
                  <!-- //////////////////////////////////////////////////////////////////////////// -->
                  <!-- START CONTENT -->
                  <section id="content">
                        <!--start container-->
                        <div class="container">
                         <div class="row">
                                    <div class="col s12">
                                          <ul class="tabs">
                                          <li class="tab col s3"><a class="active" href="#test1">Theo ngày</a></li>
                                          <li class="tab col s3"><a href="#test2">Theo tháng</a></li>
                                          <li class="tab col s3"><a href="#test3">Theo năm</a></li>
                                          </ul>
                                    </div>
                                    <div id="test1" class="col s12">
                                          <%= line_chart @total_per_day, download: true %>
                                                            <%= form_tag download_pdf_by_day_admin_path(format: "pdf"),method: :get,:id => 'form-search', target: '_new' do |f| %> 
                                                                  <div class="row">
                                                                        
                                                                        <div class="input-field col s10">
                                                                                    <%= text_field_tag :day, params[:day], :autocomplete => :off, class: "datepicker", :input_html => { :value => Date.today } %>
                                                                                    <%= label_tag 'day', 'Chọn ngày'%>
                                                                        </div>
                                                                        <div class="input-field col s2">
                                                                                    <%= button_tag(class: "btn", style: "width: 94%", id: "btn-search")  do %>
                                                                                          <i class="material-icons">search</i>
                                                                                    <%end%>
                                                                        </div> 
                                                                  </div>
                                                                        
                                                            <% end %>
                                    </div>
                                    <div id="test2" class="col s12">
                                          <%= line_chart @total_per_month, download: true %>
                                    </div>
                                    <div id="test3" class="col s12">
                                           <%= line_chart @total_per_year, download: true %>
                                           <%= form_tag download_pdf_by_year_admin_path(format: "pdf"),method: :get, target: '_new' do |f| %> 
                                                <div class="row">
                                                        
                                                        <div class="input-field col s10">
                                                                    <%= date_select("year", "written_on", selected: Date.current,discard_day: true, discard_month: true) %>
                                                                    <%= label_tag 'year', 'Tìm kiếm', class: "active"%>
                                                            </div>
                                                            <div class="input-field col s2">
                                                                    <%= button_tag(class: "btn", style: "width: 94%", id: "btn-search") do %>
                                                                        <i class="material-icons">search</i>
                                                                    <%end%>
                                                            </div> 
                                                    </div>
                                                        
                                                </div>
                                            <% end %>
                                    </div>
                              </div>
                              <!-- //////////////////////////////////////////////////////////////////////////// -->
                        </div>
                        <!--end container-->
                  </section>
                  <!-- END CONTENT -->
                  <!-- START RIGHT SIDEBAR NAV-->

                  <!-- END RIGHT SIDEBAR NAV-->
            </div>
            <!-- END WRAPPER -->
      </div>
      <!-- END MAIN -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START FOOTER -->
      <%= render 'admin_restaurants/footer' %>

