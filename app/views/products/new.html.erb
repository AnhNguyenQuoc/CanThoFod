<%= form_for @product, url: restaurant_products_new_path do |f| %>

      <%= f.label :name%>
      <%= f.text_field :name %>

      <%= f.label :description%>
      <%= f.text_field :description %>

      <%= f.label :price%>
      <%= f.number_field :price %>

      <%= f.collection_select :product_type_id, ProductType.all, :id, :name,  {prompt: "Chọn loại thức ăn"} %>

      <%= f.file_field :image%>

      <%= f.submit "Submit"%>
<% end %>
