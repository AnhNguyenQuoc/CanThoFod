 <%# <meta http-equiv="Refresh" content="30"> %>
 <!-- Start Page Loading -->
      <div id="loader-wrapper">
            <div id="loader"></div>
            <div class="loader-section section-left"></div>
            <div class="loader-section section-right"></div>
      </div>
      <!-- End Page Loading -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START HEADER -->
     <%= render 'header' %>
      <!-- END HEADER -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START MAIN -->
      <div id="main">
            <!-- START WRAPPER -->
            <div class="wrapper">
                  <!-- START LEFT SIDEBAR NAV-->
                  <%= render 'sidebar' %>
                  <!-- END LEFT SIDEBAR NAV-->
                  <!-- //////////////////////////////////////////////////////////////////////////// -->
                  <!-- START CONTENT -->
                  <section id="content">
                        <!--start container-->
                        <div class="container">
                              <!--card stats start-->
                              <div class="row">
                                    <div class="col s12">
                                          <div class="card">
                                                <div class="card-content">
                                                      <h5>Thống kê doanh thu</h5>
                                                </div>
                                                <div class="card-tabs">
                                                      <ul class="tabs tabs-fixed-width">
                                                            <li class="tab">
                                                                  <a  class="active" href="#day">Theo Ngày</a>
                                                            </li>
                                                            <li class="tab">
                                                                  <a href="#month">Theo Tháng</a>
                                                            </li>
                                                            <li class="tab">
                                                                  <a href="#year">Theo Năm</a>
                                                            </li>
                                                      </ul>
                                                </div>
                                                <div class="card-content grey lighten-4">
                                                      <div id="day"><%= line_chart @total_per_day %></div>
                                                      <div id="month"><%= line_chart @total_per_month %></div>
                                                      <div id="year"><%= line_chart @total_per_year %></div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <!-- //////////////////////////////////////////////////////////////////////////// -->
                        </div>
                        <!--end container-->
                  </section>
                  <!-- END CONTENT -->
                  <!-- START RIGHT SIDEBAR NAV-->

                  <!-- END RIGHT SIDEBAR NAV-->
            </div>
            <!-- END WRAPPER -->
      </div>
      <!-- END MAIN -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START FOOTER -->
      <%= render 'footer' %>

