 <%# <meta http-equiv="Refresh" content="30"> %>
 <!-- Start Page Loading -->
      <div id="loader-wrapper">
            <div id="loader"></div>
            <div class="loader-section section-left"></div>
            <div class="loader-section section-right"></div>
      </div>
      <!-- End Page Loading -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START HEADER -->
     <%= render 'header' %>
      <!-- END HEADER -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START MAIN -->
      <div id="main">
            <!-- START WRAPPER -->
            <div class="wrapper">
                  <!-- START LEFT SIDEBAR NAV-->
                  <%= render 'sidebar' %>
                  <!-- END LEFT SIDEBAR NAV-->
                  <!-- //////////////////////////////////////////////////////////////////////////// -->
                  <!-- START CONTENT -->
                  <section id="content">
                        <!--start container-->
                        <div class="container">
                              <!--card stats start-->
                              <div id="card-stats">
                                    <div class="row mt-1">
                                          <div class="col s12 m4">
                                                <div class="card gradient-45deg-light-blue-cyan gradient-shadow min-height-100 white-text">
                                                      <div class="padding-4">
                                                            <div class="col s7 m7">
                                                                  <i class="material-icons mt-5">add_shopping_cart</i>
                                                                  <p>Đơn hàng mới</p>
                                                            </div>
                                                            <div class="col s5 m5 right-align">
                                                                  <h5 class="mb-0"><%= @orders %></h5>
                                                                  <p class="no-margin">Tổng</p>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                          <div class="col s12 m4 ">
                                                <div class="card gradient-45deg-green-teal gradient-shadow min-height-100 white-text">
                                                      <div class="padding-4">
                                                            <div class="col s7 m7">
                                                                  <i class="material-icons mt-5">attach_money</i>
                                                                  <p>Thu nhập</p>
                                                            </div>
                                                            <div class="col s5 m5 right-align">
                                                                  <h5 class="mb-0"><%= number_to_currency_vnd(@total) %></h5>
                                                                  <p class="no-margin">Tổng</p>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                          <div class="col s12 m4">
                                                <div class="card gradient-45deg-amber-amber gradient-shadow min-height-100 white-text">
                                                      <div class="padding-4">
                                                            <div class="col s7 m7">
                                                                  <i class="material-icons mt-5">local_dining</i>
                                                                  <p>Sản phẩm</p>
                                                            </div>
                                                            <div class="col s5 m5 right-align">
                                                                  <h5 class="mb-0"><%= @products %></h5>
                                                                  <p class="no-margin">Tổng</p>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <div class="row">
                                    <div class="col s12">
                                          <div class="card">
                                                <div class="card-content">
                                                      <h5>Thống kê đơn hàng</h5>
                                                </div>
                                                <div class="card-tabs">
                                                      <ul class="tabs tabs-fixed-width">
                                                            <li class="tab">
                                                                  <a class="active" href="#test4">Theo Ngày</a>
                                                            </li>
                                                            <li class="tab">
                                                                  <a  href="#test5">Theo Tháng</a>
                                                            </li>
                                                            <li class="tab">
                                                                  <a href="#test6">Theo Năm</a>
                                                            </li>
                                                      </ul>
                                                </div>
                                                <div class="card-content grey lighten-4">
                                                      <div id="test4"><%= line_chart @order_per_day %></div>
                                                      <div id="test5"><%= line_chart @order_per_month %></div>
                                                      <div id="test6"><%= line_chart @order_per_year %></div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <!-- //////////////////////////////////////////////////////////////////////////// -->
                        </div>
                        <!--end container-->
                  </section>
                  <!-- END CONTENT -->
                  <!-- START RIGHT SIDEBAR NAV-->

                  <!-- END RIGHT SIDEBAR NAV-->
            </div>
            <!-- END WRAPPER -->
      </div>
      <!-- END MAIN -->
      <!-- //////////////////////////////////////////////////////////////////////////// -->
      <!-- START FOOTER -->
      <%= render 'footer' %>

